<h1>Your upcoming rides (<%= Booking.count %>)</h1>
<div>
  <% @bookings.each do |booking| %>
    <ul>
      <li>Booking #<%= link_to booking.id, user_booking_path(current_user, booking) %>, </li>
      <li><%= booking.vehicle.category %> | <%= booking.vehicle.model %></li>
      <li><strong>FROM</strong><%= booking.start_date %> <strong>TO</strong> <%= booking.end_date %></li>
      <li><strong>Pick-up address:</strong> <%= booking.vehicle.address %></li>
    </ul>
  <% end %>


