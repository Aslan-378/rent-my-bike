<h1>Your Bookings</h1>
<div>
  <% @bookings.each do |booking| %>
    <div>
      <p><%= link_to booking.user_id, user_booking_path(booking) %></p>
      <%= booking.vehicle.model %>
      <%= link_to user_booking_path(booking), method: :delete, data: { confirm: "Are you sure?" } do %>
        <p>Delete</p>
      <% end %>
    </div>
  <% end %>
</div>

